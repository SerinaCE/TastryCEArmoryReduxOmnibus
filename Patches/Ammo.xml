<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/CombatExtended.AmmoSetDef[
			defName="AmmoSet_357Magnum" or
			defName="AmmoSet_357Magnum_HV" or
			defName="AmmoSet_357Magnum_Revolver" or
			defName="AmmoSet_357Magnum_Lever"
			]/ammoTypes</xpath>
		<value>
			<Ammo_357Magnum_Rubber>Bullet_357Magnum_Rubber</Ammo_357Magnum_Rubber>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/DamageDef[defName="Beanbag"]/buildingDamageFactor</xpath>
		<value>
			<buildingDamageFactor>0</buildingDamageFactor>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="CE_GrenadeLauncher"]/modExtensions/li[@Class="CombatExtended.GunDrawExtension"]</xpath>
		<value>
			<DrawSize>0.93,0.93</DrawSize>
			<DrawOffset>0.2,0.0</DrawOffset>
		</value>
	</Operation>

</Patch>